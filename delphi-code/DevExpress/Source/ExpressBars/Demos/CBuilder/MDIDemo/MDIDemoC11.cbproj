<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>MDIDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;VCLX.lib;dxComnC11.lib;dxThemeC11.lib;cxLibraryC11.lib;dxBarC11.lib;dxBarExtItemsC11.lib;dxGDIPlusC11.lib;dxCoreC11.lib;cxPageControlC11.lib;dxTabbedMDIC11.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="MDIDemo.res">
    </ResFiles>
    <CppCompile Include="MDIDemo.cpp"> </CppCompile>
    <CppCompile Include="MDIDemoChild.cpp">
      <Form>frmMDIChild</Form>
    </CppCompile>
    <CppCompile Include="MDIDemoMain.cpp">
      <Form>frmMDIMain</Form>
    </CppCompile>
    <CppCompile Include="..\Common\EBarsDemoRating.cpp">
      <Form>EBarsDemoRatingForm</Form>
    </CppCompile>
    <CppCompile Include="..\Common\AboutDemoForm.cpp">
      <Form>formAboutDemo</Form>
    </CppCompile>
    <CppCompile Include="..\Common\EBarsUtils.cpp">
      <Form>dmCommonData</Form>
    </CppCompile>
    <FormResources Include="MDIDemoChild.dfm" />
    <FormResources Include="MDIDemoMain.dfm" />
    <FormResources Include="..\Common\EBarsDemoRating.dfm" />
    <FormResources Include="..\Common\AboutDemoForm.dfm" />
    <FormResources Include="..\Common\EBarsUtils.dfm" />
  </ItemGroup>
</Project>